'.source.js':

  'Sponsor Scroller JS':
    'prefix': 'sponsor-scroller'
    'rightLabelHTML': '<span style="color:#008CC3">CUSTOM</span>'
    'description': 'Scrolls through single photo elements'
    'descriptionMoreURL': 'https://github.com/sportngin/creative/wiki/Snippet-Reference-Library#sponsor-scroller'
    'body': """
        function sponsorScroll() {
            function scrollSponsors(scrollType) {
                switch (scrollType) {
                    case "start":
                        sponsorScrollerGroup.find(".pageEl:first-child").clone().appendTo(sponsorScrollerGroup),
                            sponsorScrollerGroup.css("left", 0),
                            scrollSponsorTimer = setInterval(function() {
                                $(sponsorScrollerGroup).animate({
                                    left: "-=" + sponsorScrollerWidth
                                }, 500, function() {
                                    sponsorScrollerGroup.css("left", 0),
                                        sponsorScrollerGroup.find(".pageEl:first-child").remove(),
                                        sponsorScrollerGroup.find(".pageEl:first-child").clone().appendTo(sponsorScrollerGroup)
                                })
                            }, 2e3);
                        break;
                    case "restart":
                        scrollSponsorTimer = setInterval(function() {
                            $(sponsorScrollerGroup).animate({
                                left: "-=" + sponsorScrollerWidth
                            }, 500, function() {
                                sponsorScrollerGroup.css("left", 0),
                                    sponsorScrollerGroup.find(".pageEl:first-child").remove(),
                                    sponsorScrollerGroup.find(".pageEl:first-child").clone().appendTo(sponsorScrollerGroup)
                            })
                        }, 2e3);
                        break;
                    case "stop":
                        clearTimeout(scrollSponsorTimer);
                        break;
                    case "next":
                        $(sponsorScrollerGroup).animate({
                            left: "-=" + sponsorScrollerWidth
                        }, 500, function() {
                            sponsorScrollerGroup.css("left", 0),
                                sponsorScrollerGroup.find(".pageEl:first-child").remove(),
                                sponsorScrollerGroup.find(".pageEl:first-child").clone().appendTo(sponsorScrollerGroup)
                        });
                        break;
                    case "prev":
                        sponsorScrollerGroup.find(".pageEl:last-child").remove(),
                            sponsorScrollerGroup.find(".pageEl:last-child").clone().prependTo(sponsorScrollerGroup),
                            sponsorScrollerGroup.css("left", -sponsorScrollerWidth),
                            $(sponsorScrollerGroup).animate({
                                left: "+=" + sponsorScrollerWidth
                            }, 500, function() {})
                }
            }
            if (0 === $("body.edit_mode").length) {
                var sponsorScrollerContainer = ".sponsorScroller .column-1";
                $(sponsorScrollerContainer).wrapInner('<div id="sponsorScrollerGroup"></div>');
                var scrollSponsorTimer, sponsorScrollerGroup = $(sponsorScrollerContainer).find("#sponsorScrollerGroup"),
                    sponsorScrollerWidth = sponsorScrollerGroup.find(".pageEl:first-child").outerWidth();
                $(sponsorScrollerContainer).append('<div id="sponsorScrollerNext"></div><div id="sponsorScrollerPrev"></div>'),
                    scrollSponsors("start"),
                    $(sponsorScrollerContainer).hover(function() {
                        scrollSponsors("stop")
                    }, function() {
                        scrollSponsors("restart")
                    }),
                    $("#sponsorScrollerStart").click(function() {
                        scrollSponsors("restart")
                    }),
                    $("#sponsorScrollerStop").click(function() {
                        scrollSponsors("stop")
                    }),
                    $("#sponsorScrollerNext").click(function() {
                        scrollSponsors("next")
                    }),
                    $("#sponsorScrollerPrev").click(function() {
                        scrollSponsors("prev")
                    })
            }
        }
        """

  'Multi Column JS':
    'prefix': 'multi-column'
    'rightLabelHTML': '<span style="color:#008CC3">CUSTOM</span>'
    'description': 'Makes as many columns as you want and fits them on one line'
    'descriptionMoreURL': 'https://github.com/sportngin/creative/wiki/Snippet-Reference-Library#multi-column'
    'body': """
      // Run this function on document ready and resize
        function multiColumn() {
            var $section = $('.user_mode .multi-column div.pageEl');
            var $screenWidth = $(window).width();
            //strip column styling from the multi-column CTA items and wrap in div with new class
            $section.unwrap('div');
            $section.wrap('<div class="column col-sm-12 col-md-3"></div>');
            //Adjusts the width of CTA based on number of items
            if (($screenWidth >= 1024)) {
                var numberOfItemsCTA = $('.multi-column ${1: %PAGE-ELEMENT-CLASS% }').length;
                $('.multi-column .col-md-3').css({
                    'width': 100 / numberOfItemsCTA + '%'
                });
            } else {
                $('.multi-column .col-md-3').css({
                    'width': ''
                });
            };
        }
          """

  'News Slider with Previews JS':
    'prefix': 'news-slider-previews'
    'rightLabelHTML': '<span style="color:#008CC3">CUSTOM</span>'
    'description': 'Next and previous slides are displayed on left and right'
    'descriptionMoreURL': 'https://github.com/sportngin/creative/wiki/Snippet-Reference-Library#news-slider-with-previews'
    'body': """
        $(document).on('instantiated.news_slide_show', 'body', function() {


            if ($('.slideTray').length > 0) return;

            var current = 0;
            var slide = $('.user_mode .custom-slider .newsSlideShow-article');
            var count = slide.length;

            slide.wrapAll('<div class="slideTray" />');

            var tray = $('.slideTray');

            var slidew = slide.width();
            var trayw = (slide.length + 4) * slidew;

            var start = -1 * slidew;
            var startfix = -1 * (trayw - (3 * slidew));
            var end = -1 * (trayw - (2 * slidew));
            var endfix = -1 * (slidew * 2);

            var position = start - slidew;

            tray.css('width', trayw + 'px');

            $(slide[0]).clone().addClass('clone').appendTo(tray);
            $(slide[1]).clone().addClass('clone').appendTo(tray);
            $(slide[count - 1]).clone().addClass('clone').prependTo(tray);
            $(slide[count - 2]).clone().addClass('clone').prependTo(tray);

            var timer = setInterval(function() {
                slidetray();
            }, 7000);

            nextprev();

            $('.user_mode .custom-slider .newsSlideShow-container').hover(
                function() {
                    clearInterval(timer);
                },
                function() {
                    reset();
                }
            );

            function reset(options) {

                defaults = $.extend({

                    direction: 'forward',
                    time: 7000

                }, options)

                if (defaults.direct === 'back') {
                    clearInterval(timer);
                    timer = setInterval(function() {
                        slidetray({
                            direction: defaults.direction
                        });
                    }, defaults.time);
                }

                clearInterval(timer);
                timer = setInterval(function() {
                    slidetray();
                }, defaults.time);

            }

            function nextprev() {

                $('.user_mode .custom-slider .newsSlideShow-next').click(function() {

                    slidetray();
                    reset();

                });

                $('.user_mode .custom-slider .newsSlideShow-prev').click(function() {

                    slidetray({
                        direction: 'back'
                    });
                    reset({
                        direction: 'back'
                    });

                });
            }

            function slidetray(options) {
                $('.custom-slider .newsSlideShow-article').stop();

                var defaults = $.extend({

                    direction: 'forward',
                    slot: undefined

                }, options);

                if (defaults.slot !== undefined) {

                    position = ((-1) * (defaults.slot * slidew)) - (2 * slidew);
                    tray.css('left', position + 'px');
                    reset();
                    return;
                }

                position = (defaults.direction === 'back') ? position + slidew : position - slidew;
                tray.css('left', position + 'px');

                if (position === start) {

                    setTimeout(function() {
                        something(startfix)
                    }, 970);

                } else if (position === end) {

                    setTimeout(function() {
                        something(endfix)
                    }, 970);
                }

            }

            function something(fix) {
                tray.addClass('noTransition')
                    .css('left', (fix) + 'px')
                    .focus()
                    .removeClass('noTransition');

                position = fix;
            }

            // Increase Image Quality
            $('.newsSlideShow-article img').each(function() {
                var $this = $(this);
                $this.attr('src', $this.attr('src').replace('_large', ''));
            });
        });

        """

  'Map SVG JS':
    'prefix': 'map-svg'
    'rightLabelHTML': '<span style="color:#008CC3">CUSTOM</span>'
    'description': 'Clickable USA map'
    'descriptionMoreURL': 'https://github.com/sportngin/creative/wiki/Snippet-Reference-Library#map-svg'
    'body': """
        (function($) {
          $(function() {

            function siteNum(){
              return $('html').attr('id').replace("ngin", "");
            }
          	//Set Default & Override Colors
          	 var svgMap = {
            	 usa:'/_site/images/map/usa.svg',
          	   canada:'/map/svg/canada.svg'
          	};
            var svgMapPath = 'http://assets.ngin.com/site_files/'+siteNum()+svgMap.usa;
          	var defaults = {
          	    strokeColor:"#fff",
          	    fillColor:"#bb141a",
          	    strokeWidth:1.5,
          	    fillOpacity:1,
          	    strokeopacity:1
          	};
          	var overrides = {
          	    strokeColor:"#fff",
          	    fillColor:"#180f5e",
          	    strokeWidth:1.5,
          	    fillOpacity:1,
          	    strokeopacity:1
          	};
          	//Check for code element with .svgMap custom class added to it.
          	if($('.svgMap').length == true){
            	  //Find table element with custom class .tableData added to it
            	  //Split its element id
            	  var elementId = $('.tableData').attr('id').split("_")[1];
            	  //Create jsonLink to table elements data
                jsonLink = 'http://' + window.location.host + '/page_element/ajax/' + elementId + '.json';
                //Get table JSON data and build map
          		$.get(jsonLink, function(tableData) {
                //console.log(tableData);
          			loadedLength = tableData.length;
                mobileListBuilder($('.mapOverlay'), tableData);
          			//Load SVG MAP
          			$('.svgMap').load(svgMapPath, null, function(data) {
          				//Insert Styles & HTML Elements
          				$(this).append('<div id="previewState"></div>');
          				//Variables
          			 	data = $(data);
          			 	state = data.find('path');
          			    stateLength = state.length;
          			    newSvg = new Object();


          			    //Set Defaults if Overide is Not Enabled
          			    hoverStyle = '';
          			    for(i = 0; i < stateLength; i++){
          			    	//Define vars
                      var currentState = state[i];
          					var getId = currentState.getAttribute("id");
          					//Set Defaults
          			   		currentState.setAttribute("fill", defaults.fillColor);
          			   		currentState.setAttribute("stroke-width", defaults.strokeWidth);
          			   		currentState.setAttribute("stroke", defaults.strokeColor);
          			   		currentState.setAttribute("fill-opacity", defaults.fillOpacity);
          			   		//Override
          			   		currentState.setAttribute("fill-opacity", overrides.fillOpacity);
          			   		//Add MouseOver Event
          			   		currentState.addEventListener("mouseover",function(){
          		       			var thisState = this.getAttribute("id");
          		       			var previewState = document.getElementById('previewState').style.display = 'none';
          		       		});
          			   		//Store New Object
          			   		newSvg[i] = currentState;
          			        //Compare CurrentState to LoadedStates for match and Fill State
          			        for(a = 0; a < loadedLength; a++){
          			        	var loadedState = tableData[a].state.trim().toLowerCase().replace(/\s+/g, '');
          			        	if(loadedState == getId){
          			        		hoverStyle += '#'+getId+':hover{fill-opacity:1;cursor:pointer;fill:#180f5e;}';
          			        		//State Matched Loaded Data
          			        		onData = tableData[a];
          			        		onState = onData.state;
          							onName = onData.name;
          							onLogo = onData.logo;
          							onHref = onData.href;
          							onDesc = onData.desc;
          							//Add Loaded Data to SVG
          							currentState.setAttribute("state", onState);
          							currentState.setAttribute("name", onName);
          							currentState.setAttribute("logo", onLogo);
          							currentState.setAttribute("href", onHref);
          							currentState.setAttribute("desc", onDesc);
          			            	//Set Fill Overides
          			           		currentState.setAttribute("fill", overrides.fillColor);
          			           		currentState.setAttribute("stroke-width", overrides.strokeWidth);
          			           		currentState.setAttribute("stroke", overrides.strokeColor);
          					   		currentState.setAttribute("fill-opacity", 1);
          					   		currentState.setAttribute("stroke-opacity", overrides.strokeopacity);
          					   		currentState.setAttribute("style", "path:hover{opacity:1;}");
          					   		//Apply Button Events
          			           		currentState.addEventListener("mouseover",function(e){
          			           			var thisState = this.getAttribute("id");
          			           			var thisName = this.getAttribute("name");
          			           			var thisLogo = this.getAttribute("logo");
          			           			var thisHref = this.getAttribute("href");
          			           			var thisDesc = this.getAttribute("desc");
          			           			/*var newHTML = '<a href="'+thisHref+'"><h2>'+thisName+'</h2><img src="'+thisLogo+'"/></a>';*/
                                var newHTML = '<a href="'+thisHref+'"><img src="'+thisLogo+'"/></a>';
          			           			var previewState = document.getElementById('previewState').innerHTML = newHTML;
          			           				document.getElementById('previewState').style.display = "block";
          			           		});
          			           		currentState.addEventListener("mouseout",function(){
          			           			var previewState = document.getElementById('previewState').innerHTML = '';
          			           			document.getElementById('previewState').style.display = "none";
          			           		});
          			           		click = '';
          			           		if(currentState.getAttribute("href") == '' || currentState.getAttribute("href") == '#'){
          				           		//Dont Add Click Event
          			           		}
          			           		else{
          				           		currentState.addEventListener("click",function(){
          									click++;
          									var thisHref = this.getAttribute("href");
          									var key = getId+click;
          									if(key == getId+'1'){
          										this.setAttribute("fill",overrides.fillColor);
          										window.open(thisHref,'_blank');
          									}
          									else if(key == getId+'2'){
          										this.setAttribute("fill",overrides.fillColor);
          										click = 0;
          										window.open(thisHref,'_blank');
          									}
          				           		});
          			           		}
          			           		//Store New Object
          			           		newSvg[i] = currentState;
          				        }
          			        }
          			    }
          				newStyles = '<style>'+hoverStyle+'</style>';
          			    $('.svgMap').prepend(newStyles);
          			    previewHover($('.svgMap'),get_browser());
          			    //Build New Map
          			    $('#map').empty();
          			    for(g = 0; g < stateLength; g++){
          				    $('#map').append(newSvg[g]);
          			    }
          			});
          		});
          	}
          	else{
          		//No Map
          	}
          		/* Map Functions */
          		function previewHover(doc,browser){
          			doc.mousemove(function(e){
            			//Optimize scrollTop for various borwsers
          				if(browser == "Firefox"){
          					theTop = document.documentElement.scrollTop
          				}
          				else if(browser == "Chrome" || browser == "Safari" || browser == "Opera"){
          					theTop = document.body.scrollTop;
          				}
          				else{
          					theTop = $('html').scrollTop();
          				}
          				$("#previewState").css({left:e.pageX, top:e.pageY-theTop+10});
          				e.stopPropagation();
          			});
          		 }
          		function stateClick(s,b){
          			var click = 0;
          		    s.addEventListener("mousedown",function(){
          				click++;
          				var key = b+click;
          				if(key == b+'1'){
          			    	s.setAttribute("fill",overrides.fillColor);
          				}
          				else if(key == b+'2'){
          			    	s.setAttribute("fill-opacity",overrides.fillOpacity);
          			    	click = 0;
          				}
          			},false);
          		}
          		function get_browser(){
          			var ua=navigator.userAgent,tem,M=ua.match(/(opera|chrome|safari|firefox|msie|trident(?=\/))\/?\s*(\d+)/i) || [];
          			if(/trident/i.test(M[1])){
          			    tem=/\brv[ :]+(\d+)/g.exec(ua) || [];
          			    return 'IE '+(tem[1]||'');
          			    }
          			if(M[1]==='Chrome'){
          			    tem=ua.match(/\bOPR\/(\d+)/)
          			    if(tem!=null)   {return 'Opera '+tem[1];}
          			    }
          			M=M[2]? [M[1], M[2]]: [navigator.appName, navigator.appVersion, '-?'];
          			if((tem=ua.match(/version\/(\d+)/i))!=null) {M.splice(1,1,tem[1]);}
          			return M[0];
          		}

              function mobileListBuilder(container, data){
                  container.append('<div id="mobileView"></div>');
                  for (var i = 0; i < data.length; i++) {
                    var idata = data[i];
                    $('#mobileView').append('<a id="maplist'+i+'" class="maplist'+i+'" title="'+idata.name+'" target="_blank" href="' + idata.href + '"><img src="'+idata.logo+'"/><span>'+idata.name+'</span><i>&rsaquo;</i></a>');
                  }
                }
          });
        }(jQuery));


        """




