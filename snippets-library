  'Sponsor Scroller CSS':
    'prefix': 'sponsor-scroller'
    'rightLabelHTML': '<span style="color:#008CC3">CUSTOM</span>'
    'description': 'Scrolls through single photo elements'
    'descriptionMoreURL': 'https://github.com/sportngin/creative/wiki/Snippet-Reference-Library#sponsor-scroller'
    'body': """
        // Sponsor Scoller __________________________________
          // Requirements
          // + CSS code Snippet (sponsorscroller)
          // + JS code Snippet (sponsorscroller)
          // + Add class "sponsorScoller" to a single column layout container
          // + Add single photo elements to container

              .user_mode .layoutContainer.sponsorScroller {
                  background: #fff;
                  margin: 0;
                  position: relative;
                  border-top: 1px solid $link-color;
                  border-bottom: 1px solid $link-color;
                  bottom: -15px;
                  width: 100vw;
                  margin-left: calc(-1*(100vw - 100%)/2);
                  padding: 0;

                  .pageEl {
                      display: inline-block;
                      width: 300px;
                      text-align: center;
                      padding: 0 10px;
                      vertical-align: middle;
                      box-sizing: border-box;
                      height: 80px;
                      border-right: solid 1px $link-color;
                      padding: 0 40px;
                      transition: 0.2s ease-in;

                      &:hover {
                          background: #eeeeee;

                          .pageElement.heroPhotoElement img {
                              filter: grayscale(0);
                              opacity: 1;
                          }
                      }

                      .pageElement.heroPhotoElement {
                          overflow: hidden;
                          margin: 0;
                          padding: 15px 40px;

                          img {
                              height: auto;
                              filter: grayscale(100%);
                              opacity: 0.75;
                              transition: 0.2s ease-in;
                              max-height: 50px;
                              border: none;
                          }
                      }
                  }
              }

              .user_mode .sponsorScroller .column-1 {
                  width: 100%;
                  height: 80px;
                  overflow: hidden;
                  white-space: nowrap;
                  box-sizing: border-box;
                  font-size: 0;
                  text-align: center;

                  #sponsorScrollerGroup {
                      position: relative;
                      left: 0;
                      padding: 0;
                      margin: 0 21px;
                      height: 80px;
                  }

                  #sponsorScrollerNext,
                  #sponsorScrollerPrev {
                      width: 35px;
                      height: 80px;
                      position: absolute;
                      top: 0;
                      left: 0;
                      background-color: white;
                      cursor: pointer;
                      transition: background-color 0.5s ease;
                      border-right: 1px solid $bronze;
                      border-left: 1px solid $bronze;
                      display: table;

                      &:after {
                          content: '\f100';
                          font-family: fontAwesome;
                          margin: 0 auto;
                          font-size: 30px;
                          color: $link-color;
                          display: table-cell;
                          vertical-align: middle;
                      }

                      &:hover {
                          background-color: #eeeeee;
                      }
                  }

                  #sponsorScrollerNext {
                      left: auto;
                      right: 0;

                      &:after {
                          content: '\f101';
                      }
                  }
              }
        """

  'Multi Column CSS':
    'prefix': 'multi-column'
    'rightLabelHTML': '<span style="color:#008CC3">CUSTOM</span>'
    'description': 'Makes as many columns as you want and fits them on one line'
    'descriptionMoreURL': 'https://github.com/sportngin/creative/wiki/Snippet-Reference-Library#multi-column'
