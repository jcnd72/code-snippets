       // Multi Column __________________________________
       // Requirements
       // + CSS code Snippet (multi-column)
       // + JS code Snippet (multi-column)
       // + Add class "multi-column" to a 3 column layout container
       // + Add classes to page elements you want to be affected
       .multi-column {
           position: relative;
           z-index: 1;
           display: block;
           padding: 15px;
           background-color: white;

           .column {
               flex: none;
           }

           .col-md-3,
           .col-sm-12 {
               padding: 0;
               margin: 0;
           }
       }
       @media only screen and (max-width:1024px) {
       .multi-column {
           display: flex;
           flex-wrap: wrap;

           .column {
               flex: 1 1 50%;
               border-bottom: 1px solid white;
           }
         }
       }

        @media only screen and (max-width:768px) {
             .multi-column .column {
                 flex-basis: 100%;
             }
        }
